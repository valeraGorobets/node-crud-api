{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "lib": [
      "es2018",
      "dom",
      "dom.iterable",
      "esnext.asynciterable"
    ],
  },
  "include": [
    "./**/*.ts"
  ],
  "types": [
    "node"
  ],
  "exclude":[
    "./node_modules"
  ]
}
